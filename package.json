  "scripts": {
    "seed": "nx run nest:prisma-seed --script libs/nest/prisma/seeds/main.ts",
    "docker": "docker compose build",
    "postinstall": "prisma generate --schema libs/nest/prisma/schema.prisma && yarn prebuild",
    "migrate": "prisma migrate deploy",
    "openAPI": "npx -p swagger2openapi swagger2openapi --outfile openapi.json http://0.0.0.0:7001/v1-json",
    "generate": "npx prisma generate --schema libs/nest/prisma/schema.prisma && yarn prebuild && yarn lint",
    "format": "prettier --write \"apps/cold-api/src/**/*.ts\" \"apps/cold-api/test/**/*.ts\"",
    "lint": "eslint apps/**/*.ts --fix",
    "prebuild": "yarn dlx barrelsby -D -l all --directory ./libs/react/src -d ./libs/nest/src -e ./libs/nest/src/validation/generated -e stories.tsx -e react-app-env -e private.tsx -e spec.ts -e jest.config.ts -S -H",
    "start": "nx serve",
    "start:prod": "nx serve-static",
    "storybook": "pnpm run build-storybook && npx nx storybook cold-ui",
    "build-storybook": "pnpm run prebuild && npx nx build-storybook cold-ui",
    "build": "nx run cold-api:build:development",
    "build:dev": "npx nx run cold-ui:build:development --resolveJsonModule",
    "build:prod": "npx nx run cold-ui:build:production --resolveJsonModule",
    "test": "nx run cold-api:test:ci",
    "chromatic": "npx chromatic --project-token=chpt_8c97d5f04267902 --exit-once-uploaded --onlyChanged --exit-zero-on-changes"
  },
  "prisma": {
    "generate": {
      "schema": "libs/nest/prisma/schema.prisma"
    },
    "schema": "libs/nest/prisma/schema.prisma",
    "seed": "ts-node libs/nest/prisma/seeds/main.ts"
  },
