    "seed": "prisma db seed",
    "docker": "docker compose build",
    "migrate": "prisma migrate deploy",
    "openAPI": " npx -p swagger2openapi swagger2openapi --outfile openapi.json http://0.0.0.0:7001/v1-json",
    "generate": "prisma generate --schema apps/cold-api/prisma/schema.prisma && npx barrelsby -d apps/cold-api/src/platform/modules/zod/custom -l all -D && yarn lint",
    "format": "prettier --write \"apps/cold-api/src/**/*.ts\" \"apps/cold-api/test/**/*.ts\"",
    "lint": "eslint apps/**/*.ts --fix",
    "prebuild": "npx barrelsby -D -d ./libs/react/src -l all -e stories.tsx -e react-app-env -e test.tsx -e private.tsx",
    "start": "nx serve",
    "start:prod": "nx serve-static",
    "storybook": "yarn build-storybook && npx nx storybook cold-ui",
    "build-storybook": "yarn prebuild && npx nx build-storybook cold-ui",
    "build": "yarn prebuild && npx nx build",
    "build:dev": "yarn prebuild && npx nx run cold-ui:build:development --resolveJsonModule",
    "build:prod": "yarn prebuild && npx nx run cold-ui:build:production --resolveJsonModule",
    "test": "nx test",
    "chromatic": "npx chromatic --project-token=chpt_8c97d5f04267902 --exit-once-uploaded --onlyChanged --exit-zero-on-changes"
  },
