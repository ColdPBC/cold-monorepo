  "scripts": {
    "seed": "prisma db seed",
    "docker": "docker compose build",
    "postinstall": "yarn prebuild",
    "migrate": "prisma migrate depprisloy",
    "openApi": "yarn dlx -p swagger2openapi swagger2openapi --outfile openapi.json http://0.0.0.0:7001/v1/json",
    "openApiYml": "yarn dlx -p swagger2openapi swagger2openapi --outfile openapi.yml http://0.0.0.0:7001/v1/yml",
    "generate": "yarn dlx prisma generate --schema libs/nest/prisma/schema.prisma && yarn prebuild && yarn lint",
    "format": "prettier --write \"apps/cold-api/src/**/*.ts\" \"apps/cold-api/test/**/*.ts\"",
    "lint": "eslint apps/**/*.ts --fix",
    "prebuild": "yarn barrels",
    "barrels": "yarn dlx barrelsby -D -l all --directory ./libs/react/src -d ./libs/nest/src -d apps/cold-platform-openai/src/extraction/schemas -e ./libs/nest/src/validation/generated -e stories.tsx -e react-app-env -e private.tsx -e spec.ts -e jest.config.ts -S -H",
    "start:prod": "yarn dlx nx serve-static",
    "setVersion": "node updateVersion.mjs $(git tag --sort=committerdate | tail -1 | cut -b 2-100) $(git branch --show)",
    "clearVersion": "node updateVersion.mjs '' $(git branch --show)",
    "storybook": "yarn run build-storybook && yarn dlx nx storybook cold-ui",
    "build-storybook": "yarn run prebuild && yarn dlx nx build-storybook cold-ui",
    "test-storybook": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"yarn build-storybook --quiet && npx serve dist/storybook/cold-ui -l 4400\" \"npx wait-on tcp:4400 && yarn dlx nx run cold-ui:test-storybook\"",
    "build": "yarn prebuild && nx run cold-ui:build && graphweaver build",
    "build:dev": "yarn prebuild && nx run cold-ui:build:development --resolveJsonModule",
    "build:prod": "yarn prebuild && nx run cold-ui:build:production --resolveJsonModule",
    "test": "nx run cold-api:test:ci",
    "chromatic": "yarn dlx chromatic --project-token=chpt_8c97d5f04267902 --exit-once-uploaded --onlyChanged --exit-zero-on-changes",
    "start:graph": "graphweaver start",
    "watch:graph": "graphweaver watch",
    "sync:graph": "graphweaver import postgresql --host 127.0.0.1 --password postgres --user postgres --database colddb --port 5432"
  },
  "prisma": {
    "generate": {
      "schema": "libs/nest/prisma/schema.prisma"
    },
    "schema": "libs/nest/prisma/schema.prisma",
    "seed": "ts-node libs/nest/prisma/seeds/main.ts"
  },
