{
  "name": "cold-platform-openai",
  "$schema": "/node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/cold-platform-openai/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "target": "node",
        "compiler": "tsc",
        "outputPath": "dist/apps/cold-platform-openai",
        "main": "apps/cold-platform-openai/src/main.ts",
        "tsConfig": "apps/cold-platform-openai/tsconfig.app.json",
        "assets": ["apps/cold-platform-openai/src/assets"],
        "isolatedConfig": true,
        "webpackConfig": "apps/cold-platform-openai/webpack.config.js"
      },
      "configurations": {
        "development": {},
        "staging": {},
        "production": {}
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "cold-platform-openai:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "cold-platform-openai:build:development",
          "runtimeArgs": ["--trace-warnings"],
          "port": 9004
        },
        "staging": {
          "buildTarget": "cold-platform-openai:build:staging"
        },
        "production": {
          "buildTarget": "cold-platform-openai:build:production"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/cold-platform-openai/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/cold-platform-openai/jest.config.ts"
      }
    }
  },
  "tags": []
}
