{
  "name": "cold-platform-climatiq",
  "service": {
    "name": "cold-platform-climatiq",
    "label": "OpenAI",
    "service_type": "platform",
    "definition": {
      "logo_url": "./assets/black_logo.png",
      "platform_url": "https://www.open.ai",
      "show": true,
      "facilities": false,
      "mqtt": {
        "publishes_to": {
          "user": "ui/:ENVIRONMENT:/:ORG_ID:/:EMAIL:",
          "org": "ui/:ENVIRONMENT:/:ORG_ID:/#",
          "public": "system/:ENVIRONMENT:/public",
          "cold": "system/:ENVIRONMENT:/cold"
        },
        "subscribes_to": {
          "base_topic": "/platform/openai",
          "qos": 0
        }
      },
      "rabbitMQ": {
        "rpcOptions": {
          "queue": "cold.platform.openai",
          "routing_key": "cold.platform.openai.rpc",
          "exchange": "amq.direct"
        },
        "publishOptions": {
          "queue": "cold.platform.openai",
          "routing_key": "cold.platform.openai",
          "exchange": "amq.direct"
        }
      }
    }
  },
  "version": "1.456.2"
}
