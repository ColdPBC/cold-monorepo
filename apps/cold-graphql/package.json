{
  "name": "cold-graphql",
  "version": "1.456.1",
  "description": "Cold GraphQL API",
  "scripts": {
    "build": "graphweaver build backend",
    "build:ui": "graphweaver build frontend",
    "start": "graphweaver start backend",
    "start:ui": "graphweaver start frontend",
    "watch": "graphweaver watch",
    "import": "graphweaver import postgresql -o --host localhost --password postgres --user postgres --database colddb --port 5432",
    "apply:acl": "ts-node ./custom_generators/apply_acls.ts",
    "apply:hooks": "ts-node ./custom_generators/apply_hooks.ts",
    "create:sidecar": "ts-node ./custom_generators/create_sidecar.ts",
    "sync": "yarn import && yarn generate",
    "generate": "ts-node ./custom_generators/update_database_connection.ts && yarn apply:acl && yarn apply:hooks && yarn create:sidecar"
  },
  "resolutions": {
    "graphql": "^16.10.0"
  },
  "dependencies": {
    "@as-integrations/aws-lambda": "3.1.0",
    "@auth0/auth0-spa-js": "^2.1.3",
    "@aws-sdk/client-secrets-manager": "^3.666.0",
    "@aws-sdk/credential-provider-sso": "^3.666.0",
    "@exogee/graphweaver": "2.12.2",
    "@exogee/graphweaver-admin-ui-components": "2.12.2",
    "@exogee/graphweaver-auth": "2.12.2",
    "@exogee/graphweaver-auth-ui-components": "2.12.2",
    "@exogee/graphweaver-cdk": "2.12.2",
    "@exogee/graphweaver-mikroorm": "2.12.2",
    "@exogee/graphweaver-scalars": "2.12.2",
    "@exogee/graphweaver-server": "2.12.2",
    "@mikro-orm/core": "^6.4.13",
    "@mikro-orm/knex": "^6.4.13",
    "@mikro-orm/postgresql": "^6.4.13",
    "@opentelemetry/api": "^1.9.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "axios": "^1.7.7",
    "fast-safe-stringify": "^2.1.1",
    "graphql": "^16.10.0",
    "graphweaver": "2.12.2",
    "lodash": "^4.17.21",
    "mqtt": "^5.10.1",
    "pg": "8.12.0",
    "source-map-support": "^0.5.21",
    "uuid": "^11.1.0",
    "winston": "^3.14.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "22.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "eslint --fix",
      "pre-push": "./scripts/pre-push.sh"
    }
  }
}
