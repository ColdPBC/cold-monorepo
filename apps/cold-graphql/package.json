{
    "name": "cold-graphql",
    "version": "0.1.0",
    "description": "Cold GraphQL API",
    "packageManager": "pnpm@9.8.0",
    "scripts": {
        "build": "ls -la && pwd && graphweaver build",
        "start": "ls -la && pwd && graphweaver start",
        "watch": "graphweaver watch",
        "sync": "graphweaver import postgresql --host 127.0.0.1 --password postgres --user postgres --database colddb --port 5432"
    },
    "dependencies": {
        "@as-integrations/aws-lambda": "3.1.0",
        "@auth0/auth0-spa-js": "^2.1.3",
        "@exogee/graphweaver": "2.2.7",
        "@exogee/graphweaver-admin-ui-components": "^2.2.7",
        "@exogee/graphweaver-auth": "^2.2.7",
        "@exogee/graphweaver-auth-ui-components": "^2.2.7",
        "@exogee/graphweaver-cdk": "^2.2.7",
        "@exogee/graphweaver-mikroorm": "^2.2.7",
        "@exogee/graphweaver-scalars": "2.2.7",
        "@exogee/graphweaver-server": "2.2.7",
        "@mikro-orm/core": "6.3.5",
        "@mikro-orm/knex": "6.3.5",
        "@mikro-orm/postgresql": "6.3.5",
        "graphql": "16.9.0",
        "graphweaver": "2.2.1",
        "pg": "8.12.0",
        "source-map-support": "^0.5.21"
    },
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "@types/node": "22.1.0",
        "jest": "^29.7.0",
        "lodash": "^4.17.21",
        "ts-jest": "^29.1.5",
        "ts-node": "^10.9.2",
        "typescript": "5.5.4"
    }
}
