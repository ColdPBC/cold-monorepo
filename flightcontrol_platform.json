{
  "$schema": "https://app.flightcontrol.dev/schema.json",
  "environments": [
    {
      "id": "cold-production",
      "name": "production",
      "region": "us-east-1",
      "source": {
        "pr": false,
        "branch": "main",
        "trigger": "manual"
      },
      "vpc": {
        "id": "vpc-0adff7558db4535e2",
        "cidr": "10.192.0.0/16"
      },
      "services": [
        {
          "id": "cold-platform-openai-ec2-production",
          "name": "cold-platform-openai",
          "domain": "openai.coldclimate.com",
          "port": 7003,
          "type": "web",
          "ci": {
            "type": "ec2",
            "instanceStorage": 120
          },
          "target": {
            "type": "ecs-ec2",
            "clusterInstanceSize": "t3.xlarge",
            "clusterMinInstances": 1,
            "clusterMaxInstances": 4
          },
          "cpu": 2,
          "memory": 4,
          "maxInstances": 2,
          "minInstances": 2,
          "buildType": "docker",
          "dockerfilePath": "_openai.dockerfile",
          "dockerContext": ".",
          "includeEnvVariablesInBuild": true,
          "injectEnvVariablesInDockerfile": true,
          "watchPaths": [
            "apps/cold-platform-openai/src/**",
            "libs/nest/src/**",
            "_openai.dockerfile"
          ],
          "healthCheckPath": "/health",
          "containerInsights": true,
          "enableCloudfrontSwr": false,
          "versionHistoryCount": 10,
          "dockerLabels": {
            "com.datadoghq.tags.environment": "production",
            "com.datadoghq.tags.env": "production",
            "com.datadoghq.tags.service": "cold-platform-openai",
            "com.datadoghq.ad.check_names": "[\"postgres\"]",
            "com.datadoghq.ad.init_configs": "[{}]",
            "com.datadoghq.ad.instances": "[{\"database_autodiscovery\":{\"enabled\":true},\"collect_schemas\":{\"enabled\":true},\"dbm\":true,\"host\":\"${DATABASE_URL}\",\"port\": 5432,\"username\":\"datadog\",\"password\":\"${DD_POSTGRES_PASSWORD}\", \"tags\":[\"service:cold-rds-fc-${NODE_ENV}\",\"env:${NODE_ENV}\"]",
            "com.datadoghq.tags.version": "1.0.0"
          },
          "autoscaling": {
            "cpuThreshold": 90,
            "memoryThreshold": 90,
            "cooldownTimerSecs": 300,
            "requestsPerTarget": 500
          },
          "experimental": {
            "healthCheckTimeout": 15,
            "healthCheckInterval": 35
          },
          "envVariables": {
            "DD_SERVICE": "cold-platform-openai",
            "DD_ENV": "production",
            "NODE_ENV": "production",
	          "DD_APM_ENABLED": true,
	          "DD_PROFILING_ENABLED": true,
	          "DD_PROCESS_AGENT_ENABLED": true,
	          "DD_APPSEC_AUTOMATED_USER_EVENTS_TRACKING": true,
	          "DD_APPSEC_ENABLED": true,
	          "DD_DOGSTATSD_NON_LOCAL_TRAFFIC": true,
	          "DD_DYNO_HOST": true,
	          "DD_ENABLE_DBM": true,
	          "DD_IAST_ENABLED": true,
	          "DD_LISTENERS": 2,
	          "DD_LOGS_INJECTION": true,
	          "DD_LOGS_ENABLED": true,
	          "DD_METRICS_ENABLED": true,
	          "DD_PROCESS_AGENT": true,
	          "DD_REMOTE_CONFIGURATION_ENABLED": true,
	          "DD_USE_DOGSTATSD": true
          },
          "sidecars": [
            {
              "name": "cold-datadog-openai-production",
              "image": "datadog/agent",
              "cpuAllotment": 0.5,
              "memoryAllotment": 0.25,
              "enableNetworking": true,
              "logging": true,
              "ports": [
                8125,
                5001,
                5000
              ],
              "envVariables": {
                "DD_ENV": "production",
                "NODE_ENV": "production",
                "DD_OTLP_CONFIG_RECEIVER_PROTOCOLS_GRPC_ENDPOINT": "0.0.0.0:4317",
                "DD_OTLP_CONFIG_RECEIVER_PROTOCOLS_HTTP_ENDPOINT": "0.0.0.0:4318",
                "DD_API_KEY": {
                  "fromParameterStore": "fc.cold-api-fc.env.cold-production.8pr1zak.DD_API_KEY"
                },
                "DD_AGENT_MAJOR_VERSION": "7",
                "DD_LOG_LEVEL": "ERROR",
                "DD_POSTGRES_URL_VAR": "DATABASE_URL",
                "DD_REDIS_URL_VAR": "REDISCLOUD_URL",
                "DD_SERVICE": "cold-platform-openai",
                "DD_SITE": "us5.datadoghq.com",
                "DD_APM_ENABLED": true,
                "DD_APPSEC_AUTOMATED_USER_EVENTS_TRACKING": true,
                "DD_APPSEC_ENABLED": true,
                "DD_DOGSTATSD_NON_LOCAL_TRAFFIC": true,
                "DD_METRICS_ENABLED": true,
                "DD_ENABLE_DBM": true,
                "DD_LOGS_INJECTION": true,
                "DD_LOGS_ENABLED": true,
                "DD_IAST_ENABLED": true,
                "DD_PROCESS_AGENT": true,
                "DD_PROFILING_ENABLED": true,
                "DD_REMOTE_CONFIGURATION_ENABLED": true
              }
            }
          ]
        }

      ],
      "envVariables": {
        "DD_ENV": "production",
        "NODE_ENV": "production",
	      "DD_APM_ENABLED": true,
	      "DD_PROFILING_ENABLED": true,
	      "DD_PROCESS_AGENT_ENABLED": true,
	      "DD_APPSEC_AUTOMATED_USER_EVENTS_TRACKING": true,
	      "DD_APPSEC_ENABLED": true,
	      "DD_DOGSTATSD_NON_LOCAL_TRAFFIC": true,
	      "DD_DYNO_HOST": true,
	      "DD_ENABLE_DBM": true,
	      "DD_IAST_ENABLED": true,
	      "DD_LISTENERS": 2,
	      "DD_LOGS_INJECTION": true,
	      "DD_LOGS_ENABLED": true,
	      "DD_METRICS_ENABLED": true,
	      "DD_PROCESS_AGENT": true,
	      "DD_REMOTE_CONFIGURATION_ENABLED": true,
	      "DD_USE_DOGSTATSD": true
      }
    },
    {
